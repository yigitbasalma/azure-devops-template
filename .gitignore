# Ignore node_modules directory (Node.js projects)
node_modules/

# Ignore logs and temporary files
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore environment files (often contain sensitive data)
.env
.env.local
.env.*.local

# Ignore build directories
dist/
build/
out/

# Ignore OS generated files
.DS_Store  # macOS
Thumbs.db  # Windows

# Ignore IDE or Editor settings
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln

# Ignore dependency directories for Python projects
__pycache__/
*.py[cod]

# Ignore compiled code (Java)
*.class

# Ignore compiled binaries (C, C++)
*.o
*.obj
*.so
*.exe
*.dll

# Ignore Java JAR and WAR files
*.jar
*.war

# Ignore temporary backup or swap files
*.swp
*.bak
*.tmp

# Ignore Git configuration and metadata files
.gitconfig